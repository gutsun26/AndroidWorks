<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.web.udni_sms.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_controlpanel"
        android:name="com.web.udni_sms.ui.control_panel.ControlPanelFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_controlpanel" />

    <fragment
        android:id="@+id/navigation_clients"
        android:name="com.web.udni_sms.ui.clients.ClientsFragment"
        android:label="@string/title_client"
        tools:layout="@layout/fragment_clients" />
</navigation>